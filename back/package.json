{
  "name": "back",
  "version": "1.0.0",
  "description": "API pour gestion de tournois de baby-foot",
  "main": "dist/server.js",
  "scripts": {
    "test": "dotenv -e .env.test -- npm run test:reset && jest --runInBand",
    "test:reset": "dotenv -e .env.test -- npx prisma migrate reset --force --skip-seed",
    "studio:test": "dotenv -e .env.test -- npx prisma studio",
    "dev:setup": "dotenv -e .env -- npx prisma migrate deploy",
    "studio:dev": "dotenv -e .env -- npx prisma studio",
    "dev": "dotenv -e .env -- nodemon",
    "build": "npx tsc -p tsconfig.json && node dist/server.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "nodemon": "^3.1.10",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@types/supertest": "^6.0.3",
    "dotenv-cli": "^10.0.0",
    "jest": "^30.2.0",
    "prisma": "^6.16.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
